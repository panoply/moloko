{
  "name": "moloko-monaco",
  "version": "1.0.0",
  "description": "Liquid code editing and embedded support for the browser.",
  "main": "package/moloko.js",
  "module": "package/moloko.js",
  "types": "index.d.ts",
  "type": "module",
  "bin": {
    "moloko": "./bin/moloko.cjs"
  },
  "exports": {
    "import": "./dist/moloko.js"
  },
  "scripts": {
    "dev": "concurrently \"tsup --watch\" \"http-server -o\"",
    "build": "tsup --minify --env.NODE_ENV production"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@liquify/eslint-config",
    "rules": {
      "no-use-before-define": "off"
    },
    "ignorePatterns": [
      "types/ace.d.ts",
      "*.js"
    ]
  },
  "prettier": "@liquify/prettier-config",
  "stylelint": {
    "extends": [
      "@sissel/stylelint-config"
    ],
    "ignoreFiles": [
      "public",
      "node_modules"
    ],
    "rules": {
      "selector-class-pattern": "^[a-zA-Z_-]+$",
      "rule-empty-line-before": [
        "always-multi-line",
        {
          "ignore": [
            "after-comment",
            "first-nested"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@liquify/eslint-config": "workspace:*",
    "@liquify/prettier-config": "workspace:*",
    "@sissel/stylelint-config": "^1.0.0",
    "@types/mithril": "^2.0.12",
    "@types/node": "^18.14.6",
    "ansis": "^1.5.5",
    "concurrently": "^7.6.0",
    "csstype": "^3.1.1",
    "esthetic": "workspace:0.5.5-beta.1",
    "http-server": "^14.1.1",
    "mithril": "^2.2.2",
    "monaco-editor": "^0.36.1",
    "sass": "^1.59.3",
    "stylelint": "^15.3.0",
    "stylint": "^2.0.0",
    "terser": "^5.16.5",
    "tsup": "^6.6.3"
  },
  "dependencies": {
    "bss": "^1.6.4",
    "lz-string": "^1.5.0",
    "minimist": "^1.2.8"
  },
  "peerDependencies": {
    "esthetic": "workspace:0.5.5-beta.1",
    "mithril": "^2.2.2"
  }
}
