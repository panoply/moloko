{
  "name": "moloko",
  "version": "0.2.1-beta.1",
  "description": "Moloko is an embedded web based code editor built atop of Monaco",
  "main": "dist/moloko.js",
  "module": "dist/moloko.js",
  "types": "index.d.ts",
  "type": "module",
  "bin": {
    "moloko": "./bin/moloko.cjs"
  },
  "exports": {
    "import": "./dist/moloko.js"
  },
  "scripts": {
    "dev": "concurrently \"sass ./src/styles.scss:./dist/moloko.css --watch\" \"tsup --watch\" \"http-server -o\"",
    "build": "rm -rf dist && sass ./src/styles.scss:./dist/moloko.css --no-source-map --style=compressed && NODE_ENV=production tsup --minify"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@liquify/eslint-config",
    "rules": {
      "no-use-before-define": "off"
    },
    "ignorePatterns": [
      "*.js"
    ]
  },
  "prettier": "@liquify/prettier-config",
  "stylelint": {
    "extends": [
      "@sissel/stylelint-config"
    ],
    "ignoreFiles": [
      "dist",
      "node_modules"
    ],
    "rules": {
      "string-quotes": "double",
      "selector-class-pattern": "^[a-zA-Z_-]+$",
      "rule-empty-line-before": [
        "always-multi-line",
        {
          "ignore": [
            "after-comment",
            "first-nested"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@liquify/eslint-config": "workspace:*",
    "@liquify/prettier-config": "workspace:*",
    "@sissel/stylelint-config": "^1.1.0",
    "@types/mithril": "^2.0.12",
    "@types/node": "^20.4.2",
    "ansis": "^1.5.5",
    "concurrently": "^8.2.0",
    "csstype": "^3.1.2",
    "esthetic": "workspace:^",
    "http-server": "^14.1.1",
    "mithril": "^2.2.2",
    "monaco-editor": "^0.40.0",
    "sass": "^1.63.6",
    "terser": "^5.19.0",
    "tsup": "^7.1.0"
  },
  "dependencies": {
    "@brixtol/bootstrap": "^1.6.0",
    "bss": "^1.6.4",
    "lz-string": "^1.5.0",
    "mergerino": "^0.4.0",
    "minimist": "^1.2.8",
    "url-join": "^5.0.0"
  },
  "peerDependencies": {
    "esthetic": "workspace:^",
    "mithril": "^2.2.2"
  },
  "optionalDependencies": {
    "liquidjs": "^10.8.4"
  }
}
